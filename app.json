{
  "expo": {
    "name": "Pawsome",
    "slug": "doggpt",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/intro_logo2.png",
    "scheme": [
      "pawsomeapp",
      "expopawsomeapp"
    ],
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,

    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.bondai.doggpt",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },

    "android": {
      "package": "com.pawsomebond.app",
      "googleServicesFile": "./google-services.json",
      "label": "Pawsome",
      "permissions": [
        "android.permission.BLUETOOTH",
        "android.permission.BLUETOOTH_ADMIN",
        "android.permission.BLUETOOTH_CONNECT",
        "android.permission.BLUETOOTH_SCAN",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.INTERNET"
      ],
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "pawsomeapp",
              "host": "*"
            }
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "adaptiveIcon": {
        "backgroundColor": "#0D1117",
        "foregroundImage": "./assets/images/intro_logo2.png"
      },
      "splash": {
        "image": "./assets/images/intro_logo2.png",
        "resizeMode": "contain",
        "backgroundColor": "#0D1117"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "statusBar": {
        "hidden": true,
        "translucent": true,
        "backgroundColor": "transparent"
      },
      "navigationBar": {
        "visible": "immersive",
        "behavior": "insets",
        "backgroundColor": "transparent"
      }
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/intro_logo2.png",
          "imageWidth": 280,
          "resizeMode": "contain",
          "backgroundColor": "#0D1117",
          "dark": {
            "backgroundColor": "#0D1117"
          }
        }
      ],
      "@react-native-firebase/app",
      "react-native-ble-plx",
      "expo-updates"
    ],

    "experiments": {
      "reactCompiler": true
    },

    "updates": {
      "url": "https://u.expo.dev/your-project-id",
      "enabled": true,
      "checkAutomatically": "ON_LOAD",
      "fallbackToCacheTimeout": 0
    },

    "runtimeVersion": "1.0.0",

    "extra": {
      "router": {},
      "API_BASE": "http://192.168.1.4:3000"
    }
  }
}
